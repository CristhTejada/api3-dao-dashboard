{
  "name": "api3-dao-dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build": "node dev-scripts/copy-localhost-data.js && react-scripts build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "eject": "react-scripts eject",
    "eth:compile-contracts": "cd hardhat && npx hardhat compile",
    "eth:node": "cd hardhat && npx hardhat node --network hardhat",
    "eth:run-script": "cd hardhat && npx hardhat run",
    "format": "prettier --write {src,dev-scripts}/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "lint": "eslint --ext js,ts,tsx src dev-scripts",
    "fix-dev-issues": "node dev-scripts/prepare-dao-contracts.js && node dev-scripts/scss-ts-hack.js",
    "postinstall": "yarn fix-dev-issues",
    "postuninstall": "yarn fix-dev-issues",
    "prepare": "husky install",
    "send-to-account": "cd hardhat && npx hardhat send-to-account --address",
    "start": "BROWSER=none react-scripts start",
    "test:watch": "react-scripts test",
    "test": "react-scripts test --watchAll=false",
    "tsc:watch": "tsc --project tsconfig.json --watch",
    "tsc": "tsc --project tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn tsc"
    }
  },
  "lint-staged": {
    "{src,hardhat,dev-scripts}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "package.json": "sort-package-json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@sentry/react": "^6.8.0",
    "@walletconnect/web3-provider": "^1.4.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "ethers": "^5.4.1",
    "immer": "^9.0.5",
    "lodash": "^4.17.21",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.5.2",
    "react-helmet-async": "^1.0.9",
    "react-input-autosize": "^3.0.0",
    "react-number-format": "^4.6.4",
    "react-responsive-carousel": "^3.2.19",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "web3modal": "^1.9.3"
  },
  "devDependencies": {
    "@aragon/os": "^4.4.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@openzeppelin/contracts": "^4.2.0",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/chai": "^4.2.19",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-input-autosize": "^2.2.0",
    "@types/react-router-dom": "^5.1.7",
    "api3-dao": "git+https://github.com/api3dao/api3-dao.git#85603266a3a189ccedf41b2d2f33e5911314d2f7",
    "chai": "^4.3.4",
    "chalk": "^4.1.1",
    "cypress": "^7.6.0",
    "cypress-plugin-tab": "^1.0.5",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "hardhat": "^2.4.1",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^6.0.1",
    "npx": "^10.2.2",
    "prettier": "^2.3.2",
    "react-scripts": "4.0.3",
    "replace-in-file": "^6.2.0",
    "sort-package-json": "^1.50.0",
    "ts-node": "^10.0.0",
    "typechain": "^5.1.1",
    "typescript": "^4.3.5",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
